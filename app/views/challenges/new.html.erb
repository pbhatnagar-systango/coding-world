<h1>Submit today's Challenge</h1>
<%= form_for :challenge , url: challenges_path do |f| %>
 
  <p>
    <%= f.label :Title  %><br>
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :Question  %><br>
    <%= f.text_area :question %>
  </p>
  <p>
    <%= f.label :Type  %><br>
    <%= f.select :qtype, ['REGULAR', 'C','C++','JAVA']%>

  </p>
  <p>
    <%= f.label :Level  %><br>
	<%= f.select :level, ['EASY', 'MEDIUM','HARD']%>    
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>
